<p id="notice"><%= notice %></p>

<h1>Has Used Supplies</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Supply</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @has_used_supplies.each do |has_used_supply| %>
      <tr>
        <td><%= has_used_supply.order %></td>
        <td><%= has_used_supply.supply %></td>
        <td><%= has_used_supply.quantity %></td>
        <td><%= link_to 'Show', has_used_supply %></td>
        <td><%= link_to 'Edit', edit_has_used_supply_path(has_used_supply) %></td>
        <td><%= link_to 'Destroy', has_used_supply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Has Used Supply', new_has_used_supply_path %>
