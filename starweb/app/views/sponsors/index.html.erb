            <%= link_to '', edit_sponsor_path(sponsor), class: 'fas fa-edit' %> | <%= link_to '', sponsor, method: :delete, data: {  confirm: 'EstÃ¡s seguro?' }, class: "fas fa-trash" %>
            <div class="thumbnail">
